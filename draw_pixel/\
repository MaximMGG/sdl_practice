#include "screen.h"


void line(Screen& screen, float x1, float y1, float x2, float y2) {

}

int main() {
    Screen screen;

    for(int i = 0; i < 100; i++) {
        screen.pixel(rand() % 649, rand() % 480);
    }
    while(true) {
        screen.show();
        screen.input();
    }

    return 0;
}
